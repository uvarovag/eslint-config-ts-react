    "homepage": "https://github.com/uvarovag/eslint-config-ts-react/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/uvarovag/eslint-config-ts-react.git"
    },
    "dependencies": {
        "@uvarovag/eslint-config-ts": "^1.0.16",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.1.0"
    },
    "devDependencies": {
        "@uvarovag/prettier-config": "^1.0.1",
